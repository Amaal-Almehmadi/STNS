@model STNS.Models.Parent

@{
    ViewBag.Title = "ParentDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 style="text-align:center"> تفاصيل الأم</h2>

<hr />
<div class="dl-horizontal">
    <div class="row">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.Parent_Name)
        </div>
        <div class="col-md-2">
            <span class="head">اسم الأم</span>
        </div>
    </div>

    <div class="row">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.Parent_National_Id)
        </div>
        <div class="col-md-2">
            <span class="head">الهوية الوطنية</span>
        </div>
    </div>

    <div class="row">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.Parent_Nationality)
        </div>
        <div class="col-md-2">
            <span class="head"> الجنسية</span>
        </div>
    </div>
    <div class="row">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.Parent_password)
        </div>
        <div class="col-md-2">
            <span class="head"> الرقم السري</span>
        </div>
    </div>
    <div class="row">
        <div class="col-md-10">
            @Html.DisplayFor(model => model.Parent_Username)
        </div>
        <div class="col-md-2">
            <span class="head">اسم المستخدم</span>
        </div>
    </div>


</div> <br /><br />

<p>
    @Html.ActionLink("تعديل", "Edit", new { id = Model.Parent_National_Id }) |
    @Html.ActionLink("العودة للقائمة", "ManageParent")
</p>
