@model IEnumerable<STNS.Models.Parent>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (Session["parent"] != null)
{
    <p>
        @Html.ActionLink("تعديل الحساب", "Edit")
    </p>

}
<h2 style="text-align:center">ادارة الحساب</h2>

<p>

</p>
<table class="table">
    <tr>
